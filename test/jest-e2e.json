{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/captcha": "<rootDir>/../libs/captcha/src",
    "@app/captcha/(.*)": "<rootDir>/../libs/captcha/src/$1",
    "@app/excel": "<rootDir>/../libs/excel/src",
    "@app/excel/(.*)": "<rootDir>/../libs/excel/src/$1",
    "@app/redis": "<rootDir>/../libs/redis/src",
    "@app/redis/(.*)": "<rootDir>/../libs/redis/src/$1"
  }
}